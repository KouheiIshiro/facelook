<%= form_with( model: Feed.new, local: true, url: confirm_feeds_path ) do |form| %>
  <div class="feed_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="feed_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <div class="feed_image">
    <%= form.label :image %>
    <%= form.text_field :image %>
  </div>
  <%= form.submit %>
<% end %>
